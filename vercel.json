{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "env": {
    "VITE_BASEURL": "https://bloodbank-backend-yt9o.onrender.com/api"
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
